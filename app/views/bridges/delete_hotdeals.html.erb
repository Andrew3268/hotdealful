<header><%= render 'shared/header_index' %></header>

<div class="destory_old_post">
  <p>Destory Hotdeals // <a href="/">Home</a></p>
  <table id="customers">
    <tr>
      <th>No.</th>
      <th>Title</th>
      <th>Hashtag</th>
      <th>Sub_Code</th>
      <th>Posting Time</th>
      <th>Destroy</th>
    </tr>
      
      <% @coupons.each_with_index do |coupon, i| %>
        <% if coupon.c_spare_56 == 22222 %>
            <!-- < if coupon.created_at < 5.days.ago > -->
              <tr>
                <td><p><%= i + 1 %></p></td>
                <td><p><%= coupon.c_title %></p></td>
                <td><p><%= coupon.c_hashtag %></p></td>
                <td><p><%= coupon.c_spare_57 %></p></td>
                <td><%= time_ago_in_words(coupon.created_at) %> ago</td>
                <td><%= link_to 'Destroy', coupon, method: :delete, data: { confirm: 'Are you sure?' } %></td>
              </tr>
            <!-- < end > -->
          <% end %>
        <% end %>
  </table>
 
</div>